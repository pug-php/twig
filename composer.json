    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "phug/compiler": "^0.5.2 || ^1.0.0",
        "phug/formatter": "^0.5.13 || ^1.0.0",
        "phug/lexer": "^0.5.4 || ^1.0.0",
        "phug/parser": "^0.5.2 || ^1.0.0",
        "phug/util": "^0.4.17 || ^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.15"
